<div id="<%= dom_id address %>">
  <p>
    <strong>Name:</strong>
    <%= address.name %>
  </p>

  <p>
    <strong>Address line 1:</strong>
    <%= address.address_line_1 %>
  </p>

  <p>
    <strong>Address line 2:</strong>
    <%= address.address_line_2 %>
  </p>

  <p>
    <strong>City:</strong>
    <%= address.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= address.state %>
  </p>

  <p>
    <strong>Pincode:</strong>
    <%= address.pincode %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= address.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= address.phone %>
  </p>

</div>
